local library = MrJackTable and type(MrJackTable.VenyxLibrary) == "function" and MrJackTable.VenyxLibrary()
local Settings = {}
local Window = library.new"Loomian Legacy"
Window:toggle()
-- //Main
do
	local MainPage = Window:addPage"Main"
	-- Misc Section
	do
		local MiscSection = MainPage:addSection"Misc"
		-- Active Repellent
		do
			MiscSection:addToggle("Active Repellent", Settings["Infinite Repel"], function(tog) end)
		end
		-- ignore battle npc
		do
			local NpcList = {}
			MiscSection:addToggle("Ignore NPC Battle", Settings["Ignore NPC Battle"], function(tog) Settings["Ignore NPC Battle"] = tog end)
		end
		-- Skip Dialogue
		do
			MiscSection:addToggle("Skip Dialogue", Settings["Skip Dialogue"], function(tog) Settings["Skip Dialogue"] = tog end)
		end
		-- Fast battle
		do
			MiscSection:addToggle("Fast Battle", Settings["Fast Battle"], function(tog) end)
		end
		-- Exit Battle
		do MiscSection:addButton("End Battle", function() end) end
	end
	-- GUI Section
	do
		local GUISection = MainPage:addSection"GUIs"
		GUISection:addButton("Open Rally Team", function() end)
		GUISection:addButton("Open Rallied", function() end)
		GUISection:addButton("Open PC", function() end)
		GUISection:addButton("Open Shop", function() end)
	end
end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Auto Rally
do local AutoRally_Page = Window:addPage"Auto Rally" end
-- //Other
do
	local OtherPage = Window:addPage"Other"
	-- 
	do
		local BuiltInSection = OtherPage:addSection"Built In Features"
		BuiltInSection:addButton"Skip Battle Theater Puzzles"
		BuiltInSection:addButton"Remove Progress Pop Up"
		BuiltInSection:addButton"No Unstuck CoolDown"
		BuiltInSection:addButton"Infinite UMV Battery"
	end
	local OtherSection = OtherPage:addSection"Other"
	OtherSection:addButton("Copy Discord Invite", function() end)
	OtherSection:addKeybind("Hide/Show Gui", Enum.KeyCode.RightAlt, function() Window:toggle() end)
end
local theme = Window:addPage("Theme", 5.012544693e9)
local colors = theme:addSection"Colors"
for theme, color in pairs{
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),
	TextColor = Color3.fromRGB(255, 255, 255)
} do
	-- all in one theme changer, i know, im cool
	colors:addColorPicker(theme, color, function(color3) Window:setTheme(theme, color3) end)
end
Window:toggle()
Window:SelectPage(Window.pages[1], true)
