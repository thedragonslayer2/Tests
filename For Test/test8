local Detect, SizeX, Pos, TextSize, Holders = Instance.new("ScreenGui", game:GetService"CoreGui"), 230, 250, 0, 0
if Detect.AbsoluteSize.X / SizeX < 5 then
	SizeX = math.floor(Detect.AbsoluteSize.X * 0.2)
	Pos = SizeX + 15
	TextSize = Detect.AbsoluteSize.X > 1e3 and 1 or 3
end
if Detect.AbsoluteSize.Y < 600 then Holders = 5
elseif Detect.AbsoluteSize.Y < 700 then Holders = 3 end
print(Detect.AbsoluteSize.Y)
Detect:Destroy()
