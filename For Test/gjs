local Test, Test2 = pcall(function()
	local GameName
	GameName = game:GetService"HttpService":JSONDecode(game:HttpGet("https://games.roblox.com/v1/games?universeIds=" .. game.GameId)).data[1].name
	if not GameName then
		GameName = game:GetService"MarketplaceService":GetProductInfo(game.PlaceId).Name
	end
	local Executor = identifyexecutor and identifyexecutor() or "Unknown"
	game:GetService"ReplicatedStorage".DefaultChatSystemChatEvents.SayMessageRequest:FireServer("**â€¢ " .. Executor .. ", " .. GameName .. "**", "All")
end)
if not Test then
	local ohString1 = Test2
	game:GetService"ReplicatedStorage".DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Test2, "All")
end
