local Old = game.Players.LocalPlayer.PlayerGui:FindFirstChild"ScreenGui"
if Old then Old:Destroy() end
local ScreenGui = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)
ScreenGui.ResetOnSpawn = false
local TextButton = Instance.new("TextButton", ScreenGui)
TextButton.AnchorPoint = Vector2.new(0.5, 0.5)
TextButton.Position = UDim2.new(0.5, 0, 0.543, 0)
TextButton.Size = UDim2.new(0, ScreenGui.AbsoluteSize.X * 0.075, 0, ScreenGui.AbsoluteSize.Y * 0.035)
TextButton.TextScaled = true
TextButton.Text = "Close"
TextButton.Activated:Connect(function() ScreenGui:Destroy() end)
local TextBox = Instance.new("TextBox", ScreenGui)
TextBox.AnchorPoint = Vector2.new(0.5, 0.5)
TextBox.Position = UDim2.new(0.5, 0, 0.5, 0)
TextBox.Size = UDim2.new(0, ScreenGui.AbsoluteSize.X * 0.65, 0, ScreenGui.AbsoluteSize.Y * 0.05)
TextBox.TextScaled = true
while TextBox:IsDescendantOf(game.Players.LocalPlayer.PlayerGui) do
	TextBox.Text = "https://direct-link.net/225132/622.2086564949121/dynamic/?r=aHR0cHM6Ly90aGVkcmFnb25zbGF5ZXIyLmdpdGh1Yi5pby9HZXRLZXkuaHRtbD9EVjVhX1E3RDQ3dw=="
	task.wait()
end
